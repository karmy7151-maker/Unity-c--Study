using System;
					
public class Program
{
	//제한자 반환타입 이름 매개변수로 되어있다  
	public static void Methoded(){ // 공개 정적 출력값없음(void) 이름, static이 있으면 해당클래스 안에서만 가능 없으면 밖에서도 접근가능
		Console.WriteLine("메소드 함수 실행");
	}
	
	public static int Add(int a, int b){// 공개 정적 int출력 이름 int 값 ab받음
		int c = a + b; // 함수 안에서 만든 변수는 함수내 에서만 사용가능
		return c; // 출력값이 있을경우 return으로 함수밖으로 내보낼 수 있음
		
	}
	public static int Add(int a, int b, int d){//오버로드란 같음 이름의 매개변수의 타입이나 갯수를 달리해서 정의가능
		int c = a + b+ d; 
		return c; 
		
	}
	
	public static void Main() //static 가 선언되어있을시 함수도 똑같이 선언되어있어야함
	{
		Methoded(); // 매개변수가 없을경우 이름()으로 호출
		
		string st = Add(10,20).ToString(); //매개 변수가 있을경우 이름(매개변수)로 호출하며 반환타입이 있을경우 그에맞는 변수준비 
		Console.WriteLine($"답은 {st}");
		
		st = Add(10,20,30).ToString(); 
		Console.WriteLine($"답은 {st}");
	}
}
